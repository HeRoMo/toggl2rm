{
  "name": "toggl2rm",
  "version": "2.1.0",
  "description": "A Google Sheets Add-on, which can fetch Toggl report and create time entry of Redmine.",
  "scripts": {
    "login": "./node_modules/.bin/clasp login",
    "init_gas": "./node_modules/.bin/clasp create toggl2rm --rootDir ./src",
    "push": "./node_modules/.bin/clasp push",
    "push:watch": "./node_modules/.bin/clasp push --watch"
  },
  "keywords": [
    "Toggl",
    "Redmine",
    "GAS"
  ],
  "author": "HeRoMo",
  "repository": "https://github.com/HeRoMo/toggl2rm.git",
  "license": "CC-BY-NC-4.0",
  "devDependencies": {
    "@google/clasp": "^2.4.1",
    "@types/google-apps-script": "^1.0.45",
    "tslint": "^5.11.0"
  }
}
