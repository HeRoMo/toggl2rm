{
  "name": "toggl2rm",
  "version": "1.2.2",
  "description": "A Google Sheets Add-on, which can fetch Toggl report and create time entry of Redmine.",
  "scripts": {
    "login": "./node_modules/.bin/clasp login",
    "init_gas": "./node_modules/.bin/clasp create toggl2rm && node ./utils/modify_clasp_json.js && rm ./appsscript.json",
    "build": "webpack",
    "cleanup": "rm -f dest/*",
    "deploy": "./node_modules/.bin/clasp push"
  },
  "keywords": [
    "Toggl",
    "Redmine",
    "GAS"
  ],
  "author": "HeRoMo",
  "repository": "https://github.com/HeRoMo/toggl2rm.git",
  "license": "CC-BY-NC-4.0",
  "devDependencies": {
    "@google/clasp": "^1.1.5",
    "ajv": "^6.2.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-gas": "^1.0.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-googleappsscript": "^1.0.0",
    "eslint-plugin-import": "^2.9.0",
    "file-loader": "^1.1.11",
    "gas-webpack-plugin": "^0.2.1",
    "html-webpack-plugin": "^3.0.6",
    "webpack": "^3.11.0"
  }
}
