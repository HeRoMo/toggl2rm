{
  "name": "toggl2rm",
  "version": "2.1.0",
  "description": "A Google Sheets Add-on, which can fetch Toggl report and create time entry of Redmine.",
  "scripts": {
    "eslint": "./node_modules/.bin/eslint . --ext .ts",
    "init_gas": "./node_modules/.bin/clasp create toggl2rm --rootDir ./src",
    "clasp:login": "./node_modules/.bin/clasp login",
    "clasp:push": "./node_modules/.bin/clasp push",
    "clasp:push:watch": "./node_modules/.bin/clasp push --watch"
  },
  "keywords": [
    "Toggl",
    "Redmine",
    "GAS"
  ],
  "author": "HeRoMo",
  "repository": "https://github.com/HeRoMo/toggl2rm.git",
  "license": "CC-BY-NC-4.0",
  "devDependencies": {
    "@google/clasp": "^2.4.1",
    "@types/google-apps-script": "^1.0.45",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-googleappsscript": "^1.0.4",
    "eslint-plugin-import": "^2.25.4"
  }
}
